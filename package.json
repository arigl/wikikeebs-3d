{
  "name": "keysim",
  "version": "1.3.0",
  "private": true,
  "homepage": "./",
  "main": "src/electron/main.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts test",
    "eject": "react-scripts eject",
    "prettier:fix": "prettier --write .",
    "create-colorway": "cd src/tools && bash ./createColorway.sh",
    "resize-images": "python ./src/tools/resizeImages.py",
    "compress-images": "node ./src/tools/compressImages.js",
    "generate-shadows": "node -r esm ./src/tools/generateShadows.js",
    "clean-svgs": "svgo -f ./src/assets/legends/cherry_raw -o ./src/assets/legends/cherry_cleaned && node ./src/assets/legends/convertStrokes.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^13.5.0",
    "color-convert": "^2.0.1",
    "dotenv": "^16.3.1",
    "esm": "^3.2.25",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.11.0",
    "react-scripts": "^5.0.1",
    "react-tabs": "^6.0.0",
    "react-toggle": "^4.1.2",
    "redux-subscriber": "^1.1.0",
    "sass": "^1.77.6",
    "three": "^0.119.1",
    "webfontloader": "^1.6.28",
    "wpmtest": "^1.3.9"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "canvas": "^2.11.0",
    "eslint": "^8.47.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "oslllo-svg-fixer": "^0.6.0",
    "prettier": "^2.8.8",
    "stackblur-canvas": "^2.4.0",
    "svgo": "^3.0.0"
  },
  "engines": {
    "node": "20.x"
  }
}
